#CanonicalizeHostname yes
#ServerAliveInterval 4
#ServerAliveCountMax 2
VisualHostKey=yes

Host github.com
    ControlMaster auto
    ControlPersist 120

Host *
    ControlMaster auto
    ControlPath ~/.ssh/control/%r@%h:%p
    ControlPersist 1800
    ForwardX11 no
    ForwardAgent no

Host compute.*
    PubkeyAcceptedKeyTypes +ssh-rsa

Host oci
    Hostname 144.24.93.111
    User opc

Host unli.xyz
    User jacob

Match host backup exec "timeout 0.4 nc -z 192.168.1.214 22 2>/dev/null"
    Hostname 192.168.1.214
    Port 22

Host backup
    Hostname bababushka.duckdns.org
    Port 565

Host pulse15
    Hostname 192.168.1.230

Match host pakon exec "timeout 0.4 nc -z 192.168.1.114 22 2>/dev/null"
    Hostname 192.168.1.114
    Port 22

Host pakon
    Hostname bababushka.duckdns.org
    Port 564
    User xk

Host github.com
    Hostname ssh.github.com
    Port 443
    User git
