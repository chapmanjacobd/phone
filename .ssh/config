Host github.com
        ControlMaster auto
        ControlPersist 120

Host *
    ControlMaster auto
    ControlPath ~/.ssh/control/%r@%h:%p
    ControlPersist 1800

Match host 10400 exec "timeout 2 nc -z 192.168.1.174 22 2>/dev/null"
    Hostname 192.168.1.174
    Port 22

Host 10400
    Hostname bababushka.duckdns.org
    Port 564
    User xk
